{
  "extends": ["tslint-config-egg"],
  "rules": {
    "array-bracket-spacing": false,
    "no-var-requires": false,
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  }
}
